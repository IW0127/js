{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome DevTools",
      "file": "${workspaceFolder}/index.html"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach Chrome DevTools",
      "file": "${workspaceFolder}/index.html",
      "url": "http://localhost:8000/"
    }
  ]
}
